{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "properties": {
    "outputDirectoryPath": {
      "type": "string"
    },
    "tile": {
      "type": "string"
    },
    "url": {
      "type": "string"
    },
    "mode": {
      "enum": ["development", "production"]
    },
    "resources_path": {
      "type": "string"
    },
    "plugins": {
      "type": "object",
      "additionalProperties": {
        "type": "object"
      }
    }
  },
  "required": [
    "outputDirectoryPath",
    "url"
  ]
}
