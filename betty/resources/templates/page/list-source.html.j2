{% extends 'page/base.html.j2' %}
{% set title = _('Sources') %}
{% block content %}
    {%- import 'macro/source.html.j2' as sourceMacros -%}
    <ul class="entities">
        {%- for source in entities | sort(attribute='name') %}
            <li class="{{ loop.cycle('odd', 'even') }}">
                {{ sourceMacros.link(source) }}
            </li>
        {%- endfor -%}
    </ul>
{% endblock %}
