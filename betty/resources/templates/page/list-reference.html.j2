{% extends 'page/base.html.j2' %}
{% set title = 'References' %}
{% block content %}
    {%- import 'macro/reference.html.j2' as referenceMacros -%}
    <ul class="entities">
        {%- for reference in entities | sort(attribute='name') %}
            <li class="{{ loop.cycle('odd', 'even') }}">
                {{ referenceMacros.label(reference) }}
            </li>
        {%- endfor -%}
    </ul>
{% endblock %}
